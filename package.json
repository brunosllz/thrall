{
  "name": "thrall",
  "version": "0.0.1",
  "description": "Back-end application from dev-xperience",
  "author": "Bruno Silveira Luiz",
  "private": true,
  "bin": "build/src/main.js",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc && tsc-alias && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.config.ts --detectOpenHandles --forceExit"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": [
      "build/**/*"
    ],
    "targets": [
      "node16"
    ]
  },
  "dependencies": {
    "@faker-js/faker": "8.0.2",
    "@nestjs/bull": "10.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "2.0.2",
    "@nestjs/jwt": "10.1.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "10.2.6",
    "@nestjs/platform-ws": "10.2.6",
    "@nestjs/websockets": "10.2.6",
    "@prisma/client": "5.1.1",
    "bull": "4.11.3",
    "dotenv": "^16.3.1",
    "nestjs-pino": "3.4.0",
    "nestjs-prisma": "0.22.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "pg": "^8.11.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "5.0.1",
    "rxjs": "^7.8.1",
    "socket.io": "4.7.2",
    "zod": "3.21.4",
    "zod-validation-error": "1.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.7",
    "@commitlint/config-conventional": "17.6.7",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/core": "^1.3.74",
    "@swc/jest": "^0.2.28",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "3.0.9",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import-helpers": "1.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "8.0.3",
    "jest": "^29.5.0",
    "pino-pretty": "10.2.0",
    "prettier": "^2.8.8",
    "prisma": "5.1.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
